    "gatsby-plugin-sharp": "2.0.25",
    "gatsby-plugin-typescript": "2.0.10",
    "gatsby-transformer-sharp": "2.1.15",
    "lodash": "4.17.11",
    "polished": "^3.4.2",
    "react": "16.8.4",
